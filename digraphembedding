# Here, we shall associate all the vertices of a graph with real or complex matrices.

list=[];
n=1000;
d=20;
type=Float64;
for i in 1:n
amm=[];
for j in 1:5
push!(amm,rand(1:n));
end;
push!(list,amm);
end;

eigenvector=[];
for i in 1:n
push!(eigenvector,randn(type,d));
end;

matrixtable=[];
for i in 1:n
push!(matrixtable,randn(type,d,d));
end;

# Do we want the nodes to be vectors and the edges to be matrices?

function gg(matrixtable,eigenvector)
neweigenvector=eigenvector*0;
for i in 1:n
for j in 1:length(list[i])
#neweigenvector[i]=neweigenvector[i]+matrixtable[*neweigenvector[list[i][j]]/length(list[i]);
end;
end;
return tr(neweigenvector*adjoint(eigenvector))'
end;

